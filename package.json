{
  "name": "@overra/koa-history-api-fallback",
  "version": "2.0.1",
  "description": "Provides a fallback for non-existing directories so that the HTML 5 history API can be used for Koa.",
  "keyswords": [
    "koa",
    "connect",
    "html5",
    "history api",
    "fallback",
    "spa"
  ],
  "engines": {
    "node": ">= 4"
  },
