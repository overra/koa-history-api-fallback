    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overra/koa-history-api-fallback.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-latest": "^6.16.0",
    "greenkeeper-postpublish": "^1.0.1",
    "snazzy": "^5.0.0",
    "standard": "^8.3.0"
  },
  "peerDependencies": {
    "koa": "2.x"
  },
  "files": [
    "lib"
  ]
}
